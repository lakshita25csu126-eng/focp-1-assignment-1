#include <stdio.h>
#include <ctype.h>
int main() {
    char s[1005];
    scanf("%s", s);
    for (int i = 0; s[i] != '\0'; i++) {
        if (islower(s[i]))
            s[i] = toupper(s[i]);
        else if (isupper(s[i]))
            s[i] = tolower(s[i]);
    }

    printf("%s", s);
    return 0;
}
